# Example configuration for StepSecurity GitHub Policy Store Attachments
# Copy this file to terraform.tfvars and customize the values

step_security_api_key  = "your-api-key-here"
step_security_customer = "your-customer-id"

# Path to JSON file containing policy store attachments configuration
# Default: "policy_attachments.json"
attachments_json_file = "policy_attachments.json"